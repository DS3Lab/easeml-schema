{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/v0.1/javascript/easeml-schema/play.1.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Mocha Tests",
            "program": "${workspaceFolder}/v0.1/javascript/easeml-schema/node_modules/mocha/bin/_mocha",
            "args": [
                "-u",
                "tdd",
                "--timeout",
                "999999",
                "--colors",
                "${workspaceFolder}/v0.1/javascript/easeml-schema/test",
                "--grep",
                "dataset/generate/positive/linked-003"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Python: Test Case",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/v0.1/python/easeml/schema.py",
            "args": [
                "validate",
                "v0.1/test-examples/schema/validate/negative/schema-003.json"
            ]
        },
        {
            "name": "Python: Test Case 2",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/v0.1/python/easeml/schema.py",
            "args": [
                "match",
                "v0.1/test-examples/schema/match/positive/tensor-012-src.json",
                "v0.1/test-examples/schema/match/positive/tensor-012-dst.json"
            ]
        },
        {
            "name": "Python: Test Case 3",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/v0.1/python/easeml/dataset.py",
            "args": [
                "validate",
                "v0.1/test-examples/dataset/infer/negative/linked-001"
            ]
        },
        {
            "name": "Python: Test Case 4",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/v0.1/python/easeml/dataset.py",
            "args": [
                "generate",
                "/home/bojan/temp/easeml/test001",
                "v0.1/test-examples/dataset/generate/positive/linked-002.json"
            ]
        },
        {
            "name": "Python: Test Case Dataset Infer",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/v0.1/python/easeml/test_dataset_infer.py",
            "args": [
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "Python: Attach",
            "type": "python",
            "request": "attach",
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "${workspaceFolder}",
            "port": 3000,
            "secret": "my_secret",
            "host": "localhost"
        },
        {
            "name": "Python: Terminal (integrated)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Terminal (external)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal"
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "--noreload",
                "--nothreading"
            ],
            "debugOptions": [
                "RedirectOutput",
                "Django"
            ]
        },
        {
            "name": "Python: Flask (0.11.x or later)",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ]
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "module.name"
        },
        {
            "name": "Python: Pyramid",
            "type": "python",
            "request": "launch",
            "args": [
                "${workspaceFolder}/development.ini"
            ],
            "debugOptions": [
                "RedirectOutput",
                "Pyramid"
            ]
        },
        {
            "name": "Python: Watson",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/console.py",
            "args": [
                "dev",
                "runserver",
                "--noreload=True"
            ]
        },
        {
            "name": "Python: All debug Options",
            "type": "python",
            "request": "launch",
            "pythonPath": "${config:python.pythonPath}",
            "program": "${file}",
            "module": "module.name",
            "env": {
                "VAR1": "1",
                "VAR2": "2"
            },
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "arg1",
                "arg2"
            ],
            "debugOptions": [
                "RedirectOutput"
            ]
        }
    ]
}